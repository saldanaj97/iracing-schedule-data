{
  "name": "iracing-api-interface",
  "version": "0.0.1",
  "description": "An npm package for interacting with the iRacing API",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "build": "tsup",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "ci": "pnpm run lint && pnpm run test && pnpm run build",
    "release": "pnpm run lint && pnpm run test && pnpm run build && changeset publish"
  },
  "keywords": [
    "iracing",
    "api",
    "schedule"
  ],
  "author": "Juan Saldana",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.8",
    "axios-cookiejar-support": "^5.0.1",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.5",
    "tough-cookie": "^4.1.3"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@eslint/js": "^9.0.0",
    "@jest/globals": "^29.7.0",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.6",
    "@types/tough-cookie": "^4.0.5",
    "eslint": "^9.0.0",
    "eslint-plugin-jest": "^28.2.0",
    "globals": "^15.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.4",
    "typescript-eslint": "^7.6.0"
  }
}
